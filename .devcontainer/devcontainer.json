{
  "name": "OpenDolphin WebClient Dev",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/OpenDolphin_WebClient,type=bind,consistency=cached",
  "workspaceFolder": "/workspaces/OpenDolphin_WebClient",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17"
    },
    "ghcr.io/devcontainers/features/node:1": {
      "version": "20"
    }
  },
  "forwardPorts": [
    5173,
    9080,
    9995,
    55432
  ],
  "postCreateCommand": "npm install --prefix web-client && mvn -f server-modernized/pom.xml clean install"
}
