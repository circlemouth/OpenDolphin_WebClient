# Production 向け環境変数テンプレート
# MSW は無効化し、実 API へ接続する前提。
VITE_DISABLE_MSW=1

# Production の API ベース URL
# 同一オリジンで /api リバースプロキシする場合は相対パスを推奨
VITE_API_BASE_URL=/api

# 監査ログの送信先を分離する場合は指定（未使用ならコメントアウト）
# VITE_AUDIT_ENDPOINT=https://prod.example.com/audit

# HTTP タイムアウト（ミリ秒）
VITE_HTTP_TIMEOUT_MS=10000

# ネットワークエラー時の最大リトライ回数
VITE_HTTP_MAX_RETRIES=2

# ORCA マスター取得経路（現行の bridge 実装は base URL を参照）
WEB_ORCA_MASTER_SOURCE=server
VITE_ORCA_MASTER_BRIDGE=/api
VITE_ORCA_MASTER_BRIDGE_RUN_ID=20251124T073245Z
VITE_ORCA_MASTER_SNAPSHOT_VERSION=20251124T000000Z

# 観測系（任意）
# VITE_RUM_RUN_ID=20251228T000000Z
# VITE_PERF_LOG_ENDPOINT=https://prod.example.com/__perf-log
